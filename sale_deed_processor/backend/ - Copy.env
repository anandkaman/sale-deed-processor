# backend/.env.example

# Application
APP_NAME="Sale Deed Processor"
DEBUG=True

# Database
DATABASE_URL=postgresql://postgres:admin@localhost:5432/sale_deed_db

# Paths (will be auto-created if not exist)
# BASE_DIR is auto-detected, these are relative to project root

# Tesseract Configuration
TESSERACT_LANG=eng+kan
TESSERACT_OEM=1
TESSERACT_PSM=6

# Poppler Configuration (Update for your system)
# Windows: C:\Program Files\poppler\Library\bin
# Linux: Usually in PATH, leave empty
# Mac: /usr/local/bin or leave empty
POPPLER_PATH=C:\poppler-25.11.0\Library\bin

# Poppler DPI for PDF conversion
POPPLER_DPI=300

# YOLO Model
YOLO_CONF_THRESHOLD=0.65

# Ollama Configuration
OLLAMA_BASE_URL=http://localhost:11434
LLM_MODEL=qwen2.5:3b-instruct
VISION_MODEL=qwen3-vl:4b
LLM_TEMPERATURE=0.6

# Processing Configuration
MAX_WORKERS=2
BATCH_SIZE=10

# Validation Thresholds
MIN_REGISTRATION_FEE=4000.0
MAX_MISC_FEE=4000.0